{
  "firebase": "amber-heat-8849",
  "public": "build",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "headers": [ {
    "source": "**/vendor.*.js",
    "headers": [ {
      "key": "Cache-Control",
      "value": "max-age=6048000"
    } ]
  }, {
    "source": "**/app.*.js",
    "headers": [ {
      "key": "Cache-Control",
      "value": "max-age=6048000"
    } ]
  }, {
    "source": "**/manifest.*.js",
    "headers": [ {
      "key": "Cache-Control",
      "value": "max-age=6048000"
    } ]
  } ],
  "rewrites": [ {
  "source": "**",
  "destination": "/index.html"
} ]
}
